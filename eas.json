{
  "cli": {
    "version": ">= 10.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "android": { "buildType": "apk" },
      "env": { "APP_ENV": "internal" },
      "channel": "internal",
      "developmentClient": false,
      "distribution": "internal"
    },
    "internal": {
      "android": { "buildType": "app-bundle" },
      "env": { "APP_ENV": "internal" },
      "channel": "internal",
      "developmentClient": false,
      "distribution": "internal"
    },
    "production": {
      "android": { "buildType": "app-bundle" },
      "env": { "APP_ENV": "production" },
      "channel": "production",
      "developmentClient": false
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "production"
      }
    }
  }
}
