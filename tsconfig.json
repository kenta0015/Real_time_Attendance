{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "ES2019",
    "lib": ["ES2020"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["*"],
      "@/app/*": ["app/*"],
      "@/styles/*": ["app/styles/*"],
      "@/ui/*": ["app/ui/*"],
      "@/organize/*": ["app/organize/*"],
      "@/lib/*": ["lib/*"]
    }
  }
}


